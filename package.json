{
	"name": "aw-cli",
	"version": "0.0.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"main": "dist/cli.js",
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"test": "prettier --check . && xo && ava",
		"start": "node dist/cli.js",
		"spawn": "node dist/spawn.js"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"dashify": "^2.0.0",
		"execa": "^8.0.1",
		"fuse.js": "^7.0.0",
		"glob": "^10.3.10",
		"ink": "^4.4.1",
		"ink-autocomplete": "^0.0.0",
		"ink-select-input": "^5.0.0",
		"ink-text-input": "^5.0.1",
		"meow": "^13.2.0",
		"nodemon": "^3.1.0",
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/jest": "^29.5.12",
		"@types/react": "^18.0.32",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^3.0.0",
		"jest": "^29.7.0",
		"prettier": "^2.8.7",
		"typescript": "^5.0.3",
		"xo": "^0.53.1"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	}
}
